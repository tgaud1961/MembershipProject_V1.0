@model IEnumerable<MembershipProject_V1._0.Entities.Part>
@using MembershipProject_V1._0.Areas.Admin.Models;
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.Partial("_CreateButtonPartial")
</p>
<table class="table table-responsive table-striped table-condensed">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        @Html.Partial("_TableButtonsPartial", new SmallButtonModel { Id = item.Id })
    </tr>
}

</table>
